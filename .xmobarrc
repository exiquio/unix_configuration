Config {
		font = "-schumacher-clean-bold-r-normal--12-129-75-75-c-80-*-*",
		bgColor = "#000000",
		fgColor = "#00FF00",
		position = TopW L 100,
		lowerOnStart = True,
		commands = [
			Run Cpu ["-H","80","--high","gold"] 7,
			Run CpuFreq ["-H","80","--high","gold"] 7,
			Run Memory [] 11,
			Run Swap [] 11,
			Run Network "wlan0" [] 13,
			Run Com "iwconfig wlan0 | grep -i essid | awk '{print substr($4, 7, (length($4) - 6))}'" [] "essid" 103,
			Run Battery ["-L", "35", "--low", "gold"] 17,
			Run Com "acpi | awk '{print substr($3, 0, (length($3) - 1))}'" [] "battstate" 17,
			Run Com "cat /proc/acpi/video/VGA/LCD/brightness | xargs | awk '{print $13}'" [] "lcdbrightness" 17,
			Run Weather "KSAF" ["-L","32","-H","90","--low","gold","--high","gold","-t","SF,NM: <tempF>F"] 3001,
			Run Date "%a %b %_d %H:%M" "date" 15,
			Run StdinReader
		],
		sepChar = "%",
		alignSep = "}{",
		template = "%StdinReader% }{ %cpu% | %cpufreq% | %memory% | %swap% | %wlan0% (%essid%) | %battery% (%battstate%) | LCD: %lcdbrightness% | %KSAF% | %date%"
}
